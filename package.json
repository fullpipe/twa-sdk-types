{
  "name": "twa-sdk-types",
  "version": "9.0.4",
  "description": "Types for Telegram Web Apps (TWA) SDK",
  "keywords": [
    "telegram",
    "typescript",
    "ts",
    "types",
    "telegram web apps",
    "telegram bot",
    "bot"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"rootDir\": \".\"}' mocha -r ts-node/register 'test/**/*.ts'",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run fix && git add -A src",
    "postversion": "git push && git push --tags",
    "compile": "tsc",
    "pretest": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullpipe/telegram-webapps-types.git"
  },
  "author": "Eugene Bravov <eugene.bravov@gmail.com> (https://github.com/fullpipe)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullpipe/telegram-webapps-types/issues"
  },
  "homepage": "https://github.com/fullpipe/telegram-webapps-types#readme",
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.7.5",
    "@types/sinon": "^17.0.3",
    "chai": "^5.1.2",
    "gts": "^6.0.2",
    "mocha": "^10.8.2",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "prettier": "^3.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
